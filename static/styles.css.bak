  background-color: #03b5d2;
  width: 50px;
}

.tooltip  {
  color: -webkit-link;
  cursor: pointer;
  text-decoration: underline;
}
  
.master-box {
  padding: 0 15%;
}

@media (max-width:768px){
  .master-box {
    padding: 0 0!important;
  }
}

.faq-header {
  background: linear-gradient(to right, #303f94, #4b558a);
  padding: 0 30px 10px 0;
  text-align: center;
  color: white;
}

h1.title {
  font-size: 2.1em;
  letter-spacing: -1px;
  font-weight: 400;
}

.pesquisa-faq {
  width: 80%;
  font-size: 18px;
  padding: 5px 20px 5px 20px;
  border: 1px solid #6a6a6a;
  border-radius: 10px;
  margin: 15px 0 15px 0;
}

.pesquisa-faq:focus, .pesquisa-faq:active {

  padding: 5px 20px 5px 20px;
  border: 3px solid blue;
  border-radius: 10px;
  margin: 15px 0 15px 0;
}

#myBtnContainer {
  margin: 15px;
  letter-spacing: 5px;
}

/* Style the buttons */
.btn-departamentos {
  border: none;
  outline: none;
  padding: 8px 15px 8px 0;
  background-color: #435170;
  color: #ffffffd2;
  cursor: pointer;
  font-size: 14px;
  width: 120px;
  box-shadow: 0 1px 1px 0 rgba(0,0,0,.2), 0 2px 2px 0 rgba(0,0,0,.2);
  border-radius: 0.5rem;
}

.btn-departamentos:hover {
  background: #12284c;
  color: white;
  box-shadow: 0 1px 1px 0 rgba(0,0,0,0.3), 0 3px 5px 0 rgba(0,0,0,0.3);
  transition: all 0.2s ease;
}

.btn-departamentos.active {
  background: #12284c;
  color: white;
  box-shadow: 0 1px 1px 0 rgba(0,0,0,0.3), 0 3px 5px 0 rgba(0,0,0,0.3);
  font-weight: bold;
}

.btn-departamentos::before {
  content: '\00bb   ';
  font-size: 18px;
  opacity: 0;
  transition: 0.4s;
}

.btn-departamentos.active::before {
  content: '\00bb   ';
  opacity: 1;
  transition: 0.5s;
}

#myUL {
  list-style-type: none;
  width: 100%;
  padding: 0;
  background: rgba(255, 255, 255, 0.01);
}

#myUL li span:hover:not(.header) {
    background-color: #e4ecf9;
    border: 1px solid grey;
}

.filterDiv {
  display: none; /* Hidden by default */
  transition: all 0.3s;
  margin-bottom: 4px;
}

.show {
  display: block;
}

.accordion-thumb {
  font-weight: bold;
  background: white;
}

/* Active answer */
.filterDiv.is-active .accordion-thumb::before {
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}

.accordion-thumb::before {
  content: '';
  display: inline-block;
  height: 7px;
  width: 7px;
  margin-right: 1rem;
  margin-left: .5rem;
  vertical-align: middle;
  border-right: 1px solid;
  border-bottom: 1px solid;
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
  transition: -webkit-transform .2s ease-out;
  transition: transform .2s ease-out;
  transition: transform .2s ease-out, -webkit-transform .2s ease-out;
}

.accordion-panel {
  padding: 10px 20px 10px 20px;
  display: none;
  color: black;
  font-size: 14px;
  font-weight: normal;
  background: white;
}



/* CHAT */

.nameBot{
  margin-left: 30px;
}
.chat-btn {
  position: fixed;
  right: 20px;
  cursor: pointer;
  top: 90%; /* altura que vai parar antes do topo */
  width: 50px;
  height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50px;
  background-color: #12284c;
  color: #fff;
  font-size: 25px;
  z-index: 1000;
  border: none;
  box-shadow: 0px  10px 10px rgba(0, 0, 0, 0.2);
  animation-name: pop;
	animation-duration: 0.3s;
	animation-timing-function: linear;
	animation-iteration-count: initial;
}

.chat-btn .close {
  display: none;
}

.chat-btn i {
  transition: all 0.9s ease
}

#check:checked~.chat-btn i {
  display: block;
  pointer-events: auto;
  transform: rotate(180deg)
}

#check:checked~.chat-btn .comment {
  display: none
}

.chat-btn i {
  font-size: 25px;
  color: #fff !important
}



.header-chat {
  padding: 13px;
  background-color: #12284c;
  border-radius: 5px 5px 0px 0px;
  color: #fff
}

.chat-form {
  padding: 15px
}


.form-control:focus,
.btn-chat:focus {
  box-shadow: none
}

.chat-btn:focus,
.chat-btn:hover {
  background-color: rgb(28, 28, 176);
}

#check {
  display: none !important
}

.card-response-chat{
  width: auto;
  display: flex;
  align-items: center;
}


#chatbox {
  position: relative;
  height: 400px;
  max-height: 400px;
  max-width: 300px;
  background-color:#ffffff;
  backdrop-filter: brightness(1.3) blur(5px) ;
  border-radius: 10px 0 10px 0;
